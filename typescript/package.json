{
  "name": "@azat-io/eslint-config-typescript",
  "description": "ESLint shareable config for TypeScript",
  "version": "0.8.0",
  "repository": "azat-io/eslint-config",
  "author": "Azat S. <to@azat.io>",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslint-config",
    "config",
    "code-styles",
    "linter",
    "typescript"
  ],
  "scripts": {
    "build": "vite build"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "./dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "peerDependencies": {
    "eslint": "^8.38.0"
  },
  "dependencies": {
    "@azat-io/eslint-config": "workspace:^",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint-import-resolver-typescript": "^3.5.5"
  },
  "devDependencies": {
    "eslint": "^8.41.0",
    "eslint-define-config": "^1.20.0",
    "vite": "^4.3.8",
    "vite-plugin-dts": "^2.3.0"
  }
}
