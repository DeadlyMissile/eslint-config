    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "eslint-plugin-vitest": "^0.2.2",
    "globals": "^13.20.0"
  },
  "devDependencies": {
    "eslint": "^8.41.0",
    "eslint-define-config": "^1.20.0",
    "vite": "^4.3.8",
